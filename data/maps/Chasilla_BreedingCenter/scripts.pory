script BreedingCenter_Dialogue_RevertChanges
{
    hidemoneybox
    msgbox(format("Understood. I'll revert your Pokémon back to its original state."),,SPEAKER_BREEDINGGENE)
    closemessage
    release
    end
}

script BreedingCenter_Script_StartGeneTherapy
{
    lock
    faceplayer
    msgbox(format("I specialize in Pokémon gene therapy. I help optimize a Pokemon’s Individual Values to make sure each Pokémon is performing at their peak potential."),,SPEAKER_BREEDINGGENE)
    goto(BreedingCenter_Script_AskPlayerGene)
    closemessage
    release
    end
}

