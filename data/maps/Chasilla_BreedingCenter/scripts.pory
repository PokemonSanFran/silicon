script BreedingCenter_Script_AskDiscardEgg
{
    callnative(BufferUnhatchedMonEgg)
    msgbox(BreedingCenter_Text_AreYouCertainYouDon,MSGBOX_YESNO,SPEAKER_BREEDINGEGG)

    if (var(VAR_RESULT) == YES)
