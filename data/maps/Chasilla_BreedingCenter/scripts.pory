        return
    }
    msgbox(BreedingCenter_Text_ItAppears,,SPEAKER_BREEDINGPARENT)
    msgbox(BreedingCenter_Text_PleaseMake,,SPEAKER_BREEDINGPARENT)
    goto(BreedingCenter_Script_HandleMainMenu)
}

