    release
    end
}

script BreedingCenter_Script_RemoveFundsFinishGene
{
    variableremovemoney(VAR_0x8005)
    updatemoneybox
    playse(SE_SHOP)
    waitse
    hidemoneybox
    callnative(FinalizeIndividualValueChanges)
    msgbox(format("Your changes are complete. Let me know if you want to change any more of your Pokémon."),,SPEAKER_BREEDINGEGG)
    closemessage
    release
    end
}

