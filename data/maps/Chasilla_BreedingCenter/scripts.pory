script BreedingCenter_Script_StartEggManagement
{
    lock
    faceplayer
    goto(BreedingCenter_Script_BuildEggMenu)
    release
    end
}

script BreedingCenter_Script_BuildEggMenu
{
    specialvar(VAR_RESULT,GetDaycareState)
    switch(var(VAR_RESULT))
    {
        case DAYCARE_EGG_WAITING:
            goto(BreedingCenter_Script_EggPickUpIntro)
        case DAYCARE_ONE_MON:
            call(BreedingCenter_Script_AddCheckParentsToMenu)
        case DAYCARE_TWO_MONS:
            call(BreedingCenter_Script_AddCheckParentsToMenu)
    }
    msgbox(format("I'm the Egg Manager here!"),,SPEAKER_BREEDINGEGG)
    goto(BreedingCenter_Script_HandleEggManagement)
}

script BreedingCenter_Script_AddCheckParentsToMenu
{
    dynmultipush("Check Pokémon",DAYCARE_ONE_MON)
}

script BreedingCenter_Script_AddInfoToMenu
{
    dynmultipush("Info",3)
}

script BreedingCenter_Script_HandleEggManagement
{
    call(BreedingCenter_Script_AddNeverMindToMenu)
    call(BreedingCenter_Script_AddInfoToMenu)
    msgbox(format("What can we help you with today?"),,SPEAKER_BREEDINGEGG)

    dynmultistack(0, 0, FALSE, 3, TRUE, 0, NULL)
    switch (var(VAR_RESULT))
    {
        case DAYCARE_ONE_MON: goto(BreedingCenter_Dialogue_DescribeMons)
        case 3: goto(BreedingCenter_Dialogue_InformPlayer)
        case MULTI_B_PRESSED: goto(BreedingCenter_Dialogue_EggMenuNeverMind)
    }
}

script BreedingCenter_Dialogue_EggMenuNeverMind
{
    specialvar(VAR_RESULT,GetDaycareState)
    switch(var(VAR_RESULT))
    {
        case DAYCARE_NO_MONS:
            msgbox(format("Talk to me once you've left a Pokémon with our Parent Manager."),,SPEAKER_BREEDINGEGG)
        case DAYCARE_ONE_MON:
            msgbox(format("Hopefully we'll find an Egg with your Pokemon soon."),,SPEAKER_BREEDINGEGG)
        case DAYCARE_TWO_MONS:
            msgbox(format("Hopefully we'll find an Egg with your Pokemon soon."),,SPEAKER_BREEDINGEGG)
        case DAYCARE_EGG_WAITING:
            msgbox(format("We'll hold onto this Egg until you come back. We might discover more!"),,SPEAKER_BREEDINGEGG)
    }
    closemessage
    release
    end
}

script BreedingCenter_Dialogue_InformPlayer
{
    callnative(Script_AreEffortValuesEnabled)
    msgbox(format("You can leave up to two Pokémon with our attendant inside. If your Pokémon are compatible, we'll evantually discover them with an Egg, which you can pick up from me. We'll hold up to 10 Eggs for you."),,SPEAKER_BREEDINGEGG)

    bufferspeciesname(STR_VAR_2,SPECIES_GYARADOS)
    bufferspeciesname(STR_VAR_3,SPECIES_MAGIKARP)
    msgbox(format("If you are a {STR_VAR_2} Plan member, while your Egg is waiting to be picked up, we will store your Eggs in our proprietary incubator. If you are not a {STR_VAR_2} Plan member, we invite you to apply for a {STR_VAR_2} Plan membership."),,SPEAKER_BREEDINGEGG)
    msgbox(format("While on the incubator, the kinetic energy you generate before you recieve the Egg will carry over towards hatching the Egg once you pick it up."),,SPEAKER_BREEDINGEGG)
    if (var(VAR_RESULT))
    {
        msgbox(format("The incubator also diffuses a specific vitamin solution through the Egg’s membrane, so your Pokémon’s Effort Values have been optimized. Of course, you can change them yourself too."),,SPEAKER_BREEDINGEGG)
    }

    callnative(Script_AreIndividualValuesDisabled)
    if (!var(VAR_RESULT))
    {
        msgbox(format("Finally, I specialize in DNA manipulation of unhatched Pokémon, and I can edit the Individual Values of those Eggs to your liking."),,SPEAKER_BREEDINGEGG)
    }

    callnative(BufferEggPrices)
    bufferspeciesname(STR_VAR_2,SPECIES_GYARADOS)
    bufferspeciesname(STR_VAR_3,SPECIES_MAGIKARP)

    if (flag(FLAG_TIMELINE_TRUE))
    {
        msgbox(format("As a {STR_VAR_3} Plan member,  you are responsible for the full cost of our services. Your out of pocket cost to pick up an Egg is ¥{STR_VAR_1}."),,SPEAKER_BREEDINGEGG)
        if (!var(VAR_RESULT))
        {
            callnative(BufferStatPrices)
            msgbox(format("As a {STR_VAR_3} Plan member, you are responsible for the full cost of our services. For every Individual Values you change, the cost is ¥{STR_VAR_1}."),,SPEAKER_BREEDINGEGG)
        }
    }
    else
    {
        msgbox(format("As a valued {STR_VAR_2} Plan member, 99% of your fees are covered. Your out of pocket cost to pick up an Egg is ¥{STR_VAR_1}."),,SPEAKER_BREEDINGEGG)
        if (!var(VAR_RESULT))
        {
            callnative(BufferStatPrices)
            msgbox(format("As a valued {STR_VAR_2} Plan member, 99% of your fees are covered. If you use our gene editing services, the cost for every Individual Values you change is ¥{STR_VAR_1}."),,SPEAKER_BREEDINGEGG)
        }
    }

    closemessage

    specialvar(VAR_RESULT,GetDaycareState)
    if(var(VAR_RESULT) == DAYCARE_NO_MONS)
    {
        goto(BreedingCenter_Dialogue_EggMenuNeverMind)
    }
    goto(BreedingCenter_Script_BuildEggMenu)
}

script BreedingCenter_Dialogue_DescribeMons
{
    special(GetDaycareMonNicknames)
    specialvar(VAR_RESULT,GetDaycareState)

    if(var(VAR_RESULT) == DAYCARE_ONE_MON)
    {
        msgbox(format("You left {STR_VAR_1} here. It's condition is stable, but without a partner, they won't generate an Egg."),,SPEAKER_BREEDINGEGG)
    }
    else
    {
        call(BreedingCenter_Dialogue_DescribeCompatibility)
    }
    closemessage
	release
	end
}

script BreedingCenter_Dialogue_DescribeCompatibility
{
    callnative(Script_GetDaycareCompatibilityScoreFromSave)
    switch(var(VAR_RESULT))
    {
        case PARENTS_INCOMPATIBLE:
            msgbox(format("You left {STR_VAR_1} and {STR_VAR_2} here. They have zero compatibility - we won't discover an Egg with them."),,SPEAKER_BREEDINGEGG)
        case PARENTS_LOW_COMPATIBILITY:
            msgbox(format("You left {STR_VAR_1} and {STR_VAR_2} here. They have low compatibility - it will take a while to discover an Egg."),,SPEAKER_BREEDINGEGG)
        case PARENTS_MED_COMPATIBILITY:
            msgbox(format("You left {STR_VAR_1} and {STR_VAR_2} here. They have average compatibility - we'll discover an Egg soon enough."),,SPEAKER_BREEDINGEGG)
        case PARENTS_MAX_COMPATIBILITY:
            msgbox(format("You left {STR_VAR_1} and {STR_VAR_2} here. They have extremely high compatibility - we'll discover an Egg any second now!"),,SPEAKER_BREEDINGEGG)
    }
    goto(BreedingCenter_Script_BuildEggMenu)
}

script BreedingCenter_Script_EggPickUpIntro
{
    callnative(BufferUnhatchedMonEgg)
    msgbox(format("We discovered an Egg with Pokémon you left with us. Our data suggests that it will hatch into a {STR_VAR_1}."),,SPEAKER_BREEDINGEGG)
    goto(BreedingCenter_Script_EggPickUpMenu)
}

script BreedingCenter_Script_EggPickUpMenu
{
    msgbox(format("What would you like to do with it?"),,SPEAKER_BREEDINGEGG)
    call(BreedingCenter_Script_AddTakeToMenu)
    call(BreedingCenter_Script_AddSeeOrEditToMenu)
    call(BreedingCenter_Script_AddDiscardToMenu)
    call(BreedingCenter_Script_AddResetToMenu)
    call(BreedingCenter_Script_AddCancelEggToMenu)

    dynmultistack(0, 0, FALSE, 3, TRUE, 0, NULL)

    switch (var(VAR_RESULT))
    {
        case 0: goto(BreedingCenter_Script_TakeEggSequence)
        case 1: goto(BreedingCenter_Script_ViewEgg)
        case 2: goto(BreedingCenter_Script_EditEgg)
        case 3: goto(BreedingCenter_Script_AskDiscardEgg)
        case 4: goto(BreedingCenter_Script_AskResetEgg)
        case MULTI_B_PRESSED: goto(BreedingCenter_Dialogue_EggMenuNeverMind)
    }
}

script BreedingCenter_Script_CheckSpaceForEgg
{
    callnative(Script_IsPlayerPartyAndPokemonStorageFull)

    if (var(VAR_RESULT) == FALSE)
    {
        return
    }

    msgbox(format("It appears your party and storage boxes don't have room to take this Egg!"),,SPEAKER_BREEDINGEGG)
    msgbox(format("Please make room and come back!"),,SPEAKER_BREEDINGEGG)
    closemessage
    release
    end
}

script BreedingCenter_Script_TakeEggSequence
{
    call(BreedingCenter_Script_CheckSpaceForEgg)
    showmoneybox(0,0)
    callnative(Script_CalculateAndBufferEggPickupCost)
    callnative(Script_GetDaycareCostCode)
    bufferspeciesname(STR_VAR_2,SPECIES_GYARADOS)
    bufferspeciesname(STR_VAR_3,SPECIES_MAGIKARP)

    switch(var(VAR_RESULT))
    {
        case SILICON_DAYCARE_NO_DISCOUNT_YES_STATS:
            msgbox(format("Our records indicate that you are no longer a {STR_VAR_2} Plan member. Your plan has been downgraded to the {STR_VAR_3} Plan, which does not include any discounts. Your out of pocket cost to pick up this Egg is ¥{STR_VAR_1}, which includes the cost of the Individual Values you changed. Do you still want it?"),MSGBOX_YESNO,SPEAKER_BREEDINGEGG)
        case SILICON_DAYCARE_NO_DISCOUNT_NO_STATS:
            msgbox(format("Our records indicate that you are no longer a {STR_VAR_2} Plan member. Your plan has been downgraded to the {STR_VAR_3} Plan, which does not include any discounts. Your out of pocket cost to pick up this Egg is ¥{STR_VAR_1}. Do you still want it?"),MSGBOX_YESNO,SPEAKER_BREEDINGEGG)
        case SILICON_DAYCARE_YES_DISCOUNT_YES_STATS:
            msgbox(format("As a valued {STR_VAR_2} Plan member, 99% of your fees are covered. The out of pocket cost to pick up this Egg is ¥{STR_VAR_1}, which includes the cost of the Individual Values you changed. Do you still want it?"),MSGBOX_YESNO,SPEAKER_BREEDINGEGG)
        case SILICON_DAYCARE_YES_DISCOUNT_NO_STATS:
            msgbox(format("As a valued {STR_VAR_2} Plan member, 99% of your fees are covered. Your out of pocket cost to pick up this Egg is ¥{STR_VAR_1}. Do you still want it?"),MSGBOX_YESNO,SPEAKER_BREEDINGEGG)
    }

    if(var(VAR_RESULT) == YES)
    {
        call(BreedingCenter_Script_CheckPlayerMoney)
        variableremovemoney(VAR_0x8005)
        updatemoneybox
        playse(SE_SHOP)
        waitse
        hidemoneybox
        msgbox(format("Here's your Egg!"),,SPEAKER_BREEDINGEGG)
        goto(BreedingCenter_Script_GiveEggToPlayer)
    }
    else
    {
        hidemoneybox
        goto(BreedingCenter_Script_EggPickUpMenu)
    }
}

script BreedingCenter_Script_GiveEggToPlayer
{
    closemessage
    callnative(BufferUnhatchedMonEgg)
    callnative(GiveEggFromDaycare_Silicon)
    cueobjectplayer(BREEDINGEGG)
    playfanfare(MUS_LEVEL_UP)
    if (var(VAR_0x8009) != MON_GIVEN_TO_PARTY)
    {
        bufferboxname(STR_VAR_2, VAR_PC_BOX_TO_SEND_MON)
